{
  "name": "@restaurant/backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development dotenv -e ../../.env -- tsx watch src/server.ts",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production dotenv -e ../../.env -- node dist/server.js",
    "seed": "dotenv -e ../../.env -- tsx src/scripts/seed.ts",
    "test": "cross-env NODE_ENV=test dotenv -e ../../.env -- jest",
    "test:coverage": "cross-env NODE_ENV=test dotenv -e ../../.env -- jest --coverage",
    "test:watch": "cross-env NODE_ENV=test dotenv_config_path=../../.env jest --watch"
  },
  "dependencies": {
    "@restaurant/shared": "*",
    "express": "^4.18.0",
    "mongoose": "^8.0.0",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.0",
    "cors": "^2.8.0",
    "helmet": "^7.0.0",
    "express-rate-limit": "^7.0.0",
    "compression": "^1.7.0",
    "multer": "^2.0.2",
    "cloudinary": "^1.41.0",
    "razorpay": "^2.9.0",
    "dotenv": "^16.3.0",
    "zod": "^3.22.0",
    "cookie-parser": "^1.4.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/cors": "^2.8.0",
    "@types/compression": "^1.7.0",
    "@types/multer": "^1.4.0",
    "@types/cookie-parser": "^1.4.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "supertest": "^7.1.3",
    "@types/supertest": "^6.0.0",
    "mongodb-memory-server": "^9.0.0"
  }
}
